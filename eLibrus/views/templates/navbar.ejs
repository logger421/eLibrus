<% if (typeof(user) != 'undefined') { %> 
    <% if (user.dataValues.rola == 1) { %>
        <%- include('../templates/student_navbar.ejs') %>
    <% } else if (user.dataValues.rola == 2) { %>
        <%- include('../templates/parent_navbar.ejs') %>
    <% } else if (user.dataValues.rola == 3) { %>
        <%- include('../templates/teacher_navbar.ejs') %>
    <% } else { %>
        <%- include('../templates/admin_navbar.ejs') %>
    <% } %>
<% } else { %>
    <%- include('../templates/login_navbar.ejs') %>
<% } %>